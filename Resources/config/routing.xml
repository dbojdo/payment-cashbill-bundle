<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="webit_accounting_payment_cashbill_check_transaction" path="/check-transaction">
        <default key="_controller">WebitAccountingPaymentCashbillBundle:Callback:check</default>
    </route>
    <route id="webit_accounting_payment_cashbill_payment_return" path="/return">
        <default key="_controller">WebitAccountingPaymentCashbillBundle:Callback:return</default>
    </route>
</routes>
